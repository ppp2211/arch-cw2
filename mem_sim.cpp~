#include <iostream>
#include <string>
#include <sstream>
using namespace std;

int main(int argc, char *argv[]){
	
	string line;
	
	while(getline(cin,line)){
		
		if(line.at(0) != '#'){
			
			istringstream row(line);
			string cmd;
			
			row >> cmd;
			
				if(cmd == "read-req"){
			
					unsigned addr;
					row >> addr;
					
					cout << " r ";	
				
				}
			
				else if(cmd == "write-req"){
				
					unsigned addr, data;
					row >> addr >> data;
					cout << " w ";	
				}
				
				else if(cmd == "flush-req"){
					cout << " f ";	
				}
			
				else if(cmd == "debug-req"){
					cout << " d ";	
				}
			}
		}
	
	return 0;
}
