#ifndef MEM_SIM_CACHE_HPP
#define MEM_SIM_CACHE_HPP


class cache{
	public:
		
		
		cache(int addrb, int bw, int wblk, int nblk, int nset, int clh, int clr, int clw);
		~cache();
		
		
		
		
	private:
		int addr_bit;
		int b_in_w;
		int w_in_blk;
		int blks;
		int sets;
		int cl_hit;
		int cl_read;
		int cl_write;
};


#endif
